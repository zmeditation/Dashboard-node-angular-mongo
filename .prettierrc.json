  "trailingComma": "none",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "printWidth": 141,
  "quoteProps": "as-needed",
  "bracketSpacing": true,
  "arrowParens": "always",
  "insertPragma": false,
